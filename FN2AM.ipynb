    "def reverse_string(str1):\n",
    "    reverse_str = \"\"\n",
    "    index = len(str1)\n",
    "    while index > 0:\n",
    "        reverse_str = reverse_str + str1[index - 1]\n",
    "        index = index - 1\n",
    "    return reverse_str\n",
    "Output = reverse_string('1234abcd')\n",
    "print(Output)"
   ]
